#include <stdio.h>

int main() {
    int units;

    printf("Enter total units consumed: ");
    scanf("%d", &units);

    if (units <= 100) {
        printf("Low usage");
    }
    else if (units <= 300) {
        printf("Medium usage");
    }
    else {
        printf("High usage");
    }

    return 0;
}
